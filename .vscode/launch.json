    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run main",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/main.go"
        },
        {
            "name": "Testing",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/main.go",
            "args": ["console", "open", "--profile", "secondary"]
        },
        {
            "name": "Testing clear",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/main.go",
            "args": ["credentials", "clear"]
        }
    ]
}
